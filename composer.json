{
    "name": "almirhodzic/nova-toggle-5",
    "description": "A Laravel Nova 5 toggle field that allows quick boolean updates directly from the index view.",
    "type": "library",
    "keywords": [
        "laravel",
        "nova",
        "switch",
        "field",
        "toggle",
        "boolean",
        "ui"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Almir Hodzic",
            "email": "hello@frontbyte.ch",
            "homepage": "https://frontbyte.ch"
        }
    ],
    "support": {
        "issues": "https://github.com/almirhodzic/nova-toggle-5/issues",
        "source": "https://github.com/almirhodzic/nova-toggle-5"
    },
    "homepage": "https://demo.novafront.dev",
    "require": {
        "php": "^8.2",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "AlmirHodzic\\NovaToggle5\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlmirHodzic\\NovaToggle5\\ToggleServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
